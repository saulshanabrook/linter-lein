{
  "name": "linter-lein",
  "version": "0.1.1",
  "private": false,
  "description": "Atom linter plugin for Clojure by loading each file.",
  "repository": {
    "type": "git",
    "url": "https://github.com/saulshanabrook/linter-lein"
  },
  "keywords": [
    "linter",
    "lint",
    "lein"
  ],
  "bugs": {
    "url": "https://github.com/saulshanabrook/linter-lein/issues"
  },
  "homepage": "https://github.com/saulshanabrook/linter-lein#readme",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "^4.3.2",
    "atom-package-deps": "^3.0.8"
  },
  "devDependencies": {
    "standard": "^6.0.5",
    "babel-eslint": "^4.1.5"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "package-deps": [
    "linter",
    "language-clojure"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
